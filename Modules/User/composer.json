{
  "name": "ma8h7er/user-module",
  "type": "asgard-module",
  "description": "User module for CodeEcho. Handles the authentication and authorisation as well as the user management.",
  "keywords": [
    "user",
    "authentication",
    "authorisation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maher Alhamod",
      "email": "ma8h7er@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "composer/installers": "~1.0",
    "ramsey/uuid": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "3.8.*",
    "scrutinizer/ocular": "~1.1",
    "fzaninotto/faker": "~1.5",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Modules\\User\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\User\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
