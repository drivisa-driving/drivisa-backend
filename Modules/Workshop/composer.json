{
  "name": "ma8h7er/workshop-module",
  "type": "asgard-module",
  "description": "Workshop module for CodeEcho. Handles the activation of modules.",
  "keywords": [
    "workshop",
    "modules"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maher Alhamod",
      "email": "ma8h7er@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "composer/installers": "~1.0",
    "symfony/yaml": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "3.8.*",
    "spatie/phpunit-snapshot-assertions": "^1.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Workshop\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
